{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "../public/js/",
	"rootDir": "./app"
  },
  "files":[
    "app/**/*.ts"
  ],
  "exclude":[
    "node-modules"
  ]
}